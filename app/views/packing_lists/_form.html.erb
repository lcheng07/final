<%= form_for [@trips, @packing_list] do |form| %>
  <div class="form-group">
    <%= form.label :item %>
    <%= form.collection_select :item_id, Item.order(:name), :id, :name, { include_blank: true }, { class: "form-control" } %>
  </div>
  <%= form.button title, class: "btn btn-success" %>
<% end %>
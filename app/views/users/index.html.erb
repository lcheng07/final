<div class="jumbotron">
	<h1>My Account</h1>
</div>

<div class="row">

	<% @users.each do |user| %>
   <h3><%= user.name %> </h3>
   	<h5><%= link_to "Edit", edit_user_path(user), class: "btn btn-success" %> <%= link_to "Delete", user_path(user), method: :delete, data: { confirm: 'Are you really really really sure?' }, class: "btn btn-success"%></h5>
  <% end %>

</div>
<div class="jumbotron">
	<h1>All Users</h1>
</div>


<% @users.each do |user| %>

<ul>
	<li><%= link_to user.username, user_path(user)%> <%= link_to "Edit", edit_user_path(user)%> <%= link_to "Delete", user_path(user), method: :delete, data: {confirm: 'Are you sure?'}%>
		<p><%= user.first_name%> <%=user.last_name%></p>
	</li>


</ul>		
<% end %>


<%= link_to "New User", new_user_path, class: 'btn btn-success' %>


<div class="jumbotron">
	<h1><%=@users.name%></h1>
</div>



<div class="row">
	<h3>Name: </h3>
		<h5><%= current_user.name %></h5>
	<h3>Username: </h3>
		<h5><%= current_user.username %></h5>
	<h3>Email: </h3>	
		<h5><%= current_user.email %></h5>
	<h3>User Created:</h3> 
		<h5><%=@users.created_at%></h5> 
	<h3>User Updated:</h3> 
		<h5><%=@users.updated_at%></h5> 

	<%= link_to "Edit User", edit_user_path, class: "btn btn-success" %>	
	<%= link_to "Delete User", user_path, method: :delete, data: { confirm: 'Are you really really really sure?' }, class: "btn btn-success" %>

</div>